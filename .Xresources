!SETTINGS
*font:       -kakwa-kakwafont-medium-r-normal--12-120-72-72-c-60-iso8859-1
*boldFont:   -kakwa-kakwafont-medium-r-normal--12-120-72-72-c-60-iso8859-1
*italicFont: -kakwa-kakwafont-medium-r-normal--12-120-72-72-c-60-iso8859-1 

!COLORS
*highlightColor: #242529
*cursorColor: 	 #DBDBDB
*borderColor: 	 #1C1D1F
*background:  	 #1C1D1F
*foreground:  	 #F2ECEC

!BLACK
*color0:  #202124
*color8:  #242529
!RED
*color1:  #E8E2E2
*color9:  #FCFCFC
!GREEN
*color2:  #AADB0F
*color10: #BAF010
!YELLOW
*color3:  #2E3233
*color11: #404647
!BLUE
*color4:  #E2E2E2
*color12: #F7F7F7
!MAGENTA
*color5:  #91BA0D
*color13: #A1CF0E
!CYAN
*color6:  #B2B2B2
*color14: #C7C7C7
!WHITE
*color7:  #D6D0D0
*color15: #DBDBDB

!URXVT SETTINGS
URxvt*dynamicColors:   on
URxvt.internalBorder:  20
URxvt*lineSpace:       2
URxvt*scrollBar:       false
URxvt.geometry:        74x16
URxvt*saveLines:       1000
URxvt*cursorUnderline: false
URxvt*cursorBlink:     true
URxvt*allow_bold:      true

!PERL
URxvt*perl-lib:        /usr/lib/urxvt/perl/
URxvt*perl-ext-common: default,matcher

!CLICKABLE LINKS
URxvt*url-launcher:   inox
URxvt*matcher.button: 1

!2BWM COLORS
twobwm.border_width: 	   6
twobwm.outer_border: 	   0
twobwm.focus_color: 	   #F2ECEC
twobwm.unfocus_color: 	   #292A2E
twobwm.fixed_color: 	   #292A2E
twobwm.unkill_color: 	   #292A2E
twobwm.outer_border_color: #1C1D1F
twobwm.fixed_unkill_color: #292A2E

!SXIV COLORS
sxiv.font: 		   kakwafont:size=8
sxiv.win_bg_color: #1C1D1F
sxiv.win_fs_color: #242529
sxiv.sel_color:    #242529
sxiv.bar_bg_color: #202124
sxiv.bar_fg_color: #F2ECEC
sxiv.win_width:    460
sxiv.win_height:   350

!MISC
Xcursor*size: 16
