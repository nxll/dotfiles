!SETTINGS
*font:       -kakwa-kakwafont-medium-r-normal--12-120-72-72-c-60-iso8859-1
*boldFont:   -kakwa-kakwafont-medium-r-normal--12-120-72-72-c-60-iso8859-1
*italicFont: -kakwa-kakwafont-medium-r-normal--12-120-72-72-c-60-iso8859-1 

!COLORS
*background:  #21211F
*foreground:  #B8B9B4

!BLACK
*color0:      #262624
*color8:      #2B2B29
!RED
*color1:      #939490
*color9:      #989995
!GREEN
*color2:      #5E5E5C
*color10:     #636361
!YELLOW
*color3:      #B6B8B3
*color11:     #BBBDB8
!BLUE
*color4:      #727370
*color12:     #777875
!MAGENTA
*color5:      #D1D1CB
*color13:     #D6D6D0
!CYAN
*color6:      #636361
*color14:     #696966
!WHITE
*color7:      #AAABA6
*color15:     #AFB0AB

*highlightColor:     #2B2B29
*borderColor:        #21211F
*cursorColor:        #AFB0AB

!URXVT SETTINGS
URxvt*dynamicColors:      on
URxvt.internalBorder:     15
URxvt*lineSpace:          2
URxvt*scrollBar:          false
URxvt.geometry:           74x16
URxvt*saveLines:          1000
URxvt*cursorUnderline:    false
URxvt*cursorBlink:        true
URxvt*allow_bold:         true

!PERL
URxvt*perl-lib:        /usr/lib/urxvt/perl/
URxvt*perl-ext-common: default,matcher

!CLICKABLE LINKS
URxvt*url-launcher:   inox
URxvt*matcher.button: 1

!2BWM COLORS
twobwm.border_width: 	   5
twobwm.outer_border: 	   2
twobwm.focus_color: 	   #A8A8A6
twobwm.unfocus_color: 	   #353533
twobwm.fixed_color: 	   #353533
twobwm.unkill_color: 	   #353533
twobwm.outer_border_color: #21211F
twobwm.fixed_unkill_color: #353533

!SXIV COLORS
sxiv.font: 			kakwafont:size=8
sxiv.win_bg_color: 	#21211F
sxiv.win_fs_color: 	#2B2B29
sxiv.sel_color: 	#2B2B29
sxiv.bar_bg_color: 	#262624
sxiv.bar_fg_color: 	#B8B9B4
sxiv.win_width: 	460
sxiv.win_height: 	350

!MISC
Xcursor*size:  		  16
