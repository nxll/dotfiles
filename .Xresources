!SETTINGS
*font:       -jmk-neep-medium-r-semicondensed--11-100-75-75-c-50-iso8859-1
*boldFont:   -jmk-neep-medium-r-semicondensed--11-100-75-75-c-50-iso8859-1
*italicFont: -jmk-neep-medium-r-semicondensed--11-100-75-75-c-50-iso8859-1 

!COLORS
*highlightColor: #282826
*cursorColor: 	 #AFB0AB
*borderColor: 	 #21211F
*background:  	 #21211F
*foreground:  	 #B8B9B4

!BLACK
*color0:  #262624
*color8:  #282826
!RED
*color1:  #939490
*color9:  #989995
!GREEN
*color2:  #9AC70E
*color10: #C2FA11
!YELLOW
*color3:  #B6B8B3
*color11: #BBBDB8
!BLUE
*color4:  #727370
*color12: #777875
!MAGENTA
*color5:  #D1D1CB
*color13: #D6D6D0
!CYAN
*color6:  #636361
*color14: #696966
!WHITE
*color7:  #AAABA6
*color15: #AFB0AB

!URXVT SETTINGS
URxvt*dynamicColors:   on
URxvt.internalBorder:  20
URxvt*lineSpace:       2
URxvt*scrollBar:       false
URxvt.geometry:        74x16
URxvt*saveLines:       1000
URxvt*cursorUnderline: false
URxvt*cursorBlink:     true
URxvt*allow_bold:      true

!PERL
URxvt*perl-lib:        /usr/lib/urxvt/perl/
URxvt*perl-ext-common: default,matcher

!CLICKABLE LINKS
URxvt*url-launcher:   firefox
URxvt*matcher.button: 1

!2BWM COLORS
twobwm.border_width: 	   5
twobwm.outer_border: 	   0
twobwm.focus_color: 	   #282826
twobwm.unfocus_color: 	   #262624
twobwm.fixed_color: 	   #939490
twobwm.unkill_color: 	   #21211F
twobwm.outer_border_color: #21211F
twobwm.fixed_unkill_color: #989995
twobwm.inverted_colors:    false
twobwm.enable_compton: 	   true

!SXIV COLORS
sxiv.font: 		   jmk neep:size=8
sxiv.win_bg_color: #21211F
sxiv.win_fs_color: #282826
sxiv.sel_color:    #282826
sxiv.bar_bg_color: #262624
sxiv.bar_fg_color: #B8B9B4
sxiv.win_width:    460
sxiv.win_height:   350

!MISC
Xcursor*size: 16
