!SETTINGS
*font:       -kakwa-kakwafont-medium-r-normal--12-120-72-72-c-60-iso8859-1
*boldFont:   -kakwa-kakwafont-medium-r-normal--12-120-72-72-c-60-iso8859-1
*italicFont: -kakwa-kakwafont-medium-r-normal--12-120-72-72-c-60-iso8859-1 

!COLORS
*highlightColor:     #57607A
*borderColor:        #3C4254
*cursorColor:        #FDCBAC

*background:  #3C4254
*foreground:  #FEDDD0

!BLACK
*color0:      #4B5269
*color8:      #57607A
!RED
*color1:      #D8686D
*color9:      #ED7278
!GREEN
*color2:      #918EB3
*color10:     #A19EC7
!YELLOW
*color3:      #EBC6A1
*color11:     #FFD7AF
!BLUE
*color4:      #B84E46
*color12:     #CC564E
!MAGENTA
*color5:      #875F87
*color13:     #9C6D9C
!CYAN
*color6:      #F77C6C
*color14:     #FF806F
!WHITE
*color7:      #FDCBAC
*color15:     #FDCBAC

!URXVT SETTINGS
URxvt*dynamicColors:      on
URxvt.internalBorder:     15
URxvt*lineSpace:          2
URxvt*scrollBar:          false
URxvt.geometry:           74x16
URxvt*saveLines:          1000
URxvt*cursorUnderline:    false
URxvt*cursorBlink:        true
URxvt*allow_bold:         true

!PERL
URxvt*perl-lib:        /usr/lib/urxvt/perl/
URxvt*perl-ext-common: default,matcher

!CLICKABLE LINKS
URxvt*url-launcher:   inox
URxvt*matcher.button: 1

!2BWM COLORS
twobwm.border_width: 	   6
twobwm.outer_border: 	   3
twobwm.focus_color: 	   #D8686D
twobwm.unfocus_color: 	   #4B5269
twobwm.fixed_color: 	   #4B5269
twobwm.unkill_color: 	   #4B5269
twobwm.outer_border_color: #3C4254
twobwm.fixed_unkill_color: #4B5269

!MISC
Xcursor*theme: 		  pixelfun
Xcursor*size:  		  16
